{
  "schemaVersion": 1,
  "id": "wtb",
  "version": "${version}",

  "name": "Wami's Toolbox!",
  "description": "Wami's Toolbox adds many random things, stuff like super relaxed difficulty and better sinew recipes!",
  "authors": [
    "Umngane Wami"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "wamis.tool.box.WamisToolBox"
    ],
    "preLaunch": [
      "wamis.tool.box.ExamplePreLaunchInitializer"
    ]
  },
  "mixins": [
    "wtb.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
}
